  "description": "package to manage ads",
  "main": "./dist/index.js",
  "peerDependencies": {
    "prop-types": "^15.x",
    "react": "^16.x",
    "react-dom": "^16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@webdeveloperpr/prebid": "github:webdeveloperpr/prebid#master",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.7.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "np": "^3.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "regenerator-runtime": "^0.13.1",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.js",
    "start": "webpack-dev-server --mode development && npm run build:watch",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "clean-dir": "rm -rf ./dist",
    "build": "npm run clean-dir && NODE_ENV=production babel src --out-dir dist --ignore ./src/**/*.spec.js",
    "build:prod": "npm run clean-dir && NODE_ENV=production babel src --out-dir dist --ignore ./src/**/*.spec.js --presets minify",
    "build:watch": "npm run clean-dir && NODE_ENV=production babel --watch src --out-dir dist --ignore ./src/**/*.spec.js",
    "release": "np"
  },
  "keywords": [
    "react",
    "prebid",
    "ads",
    "gpt",
    "dfp",
    "header",
    "bidder",
    "google"
  ],
  "author": "Luis Betancourt",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdeveloperpr/react-ads.git"
  },
  "bugs": {
    "url": "https://github.com/webdeveloperpr/react-ads/issues"
  },
  "homepage": "https://github.com/webdeveloperpr/react-ads#readme",
  "dependencies": {
    "lodash.debounce": "^4.0.8"
  }
